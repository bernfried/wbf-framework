package de.webertise.ds.manager.profilemgr.config;

public class PMOInputParameter {
	
	private String name;
	private String standardValue;
	private boolean mandatory;
	private String urlEncoding;
	private String htmlEncoding;
	private boolean persistent;
	private int type;
	private String inputFormat;
	private String persistFormat;
	
	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}
	/**
	 * @return the standardValue
	 */
	public String getStandardValue() {
		return standardValue;
	}
	/**
	 * @return the mandatory
	 */
	public boolean isMandatory() {
		return mandatory;
	}
	/**
	 * @return the urlEncoding
	 */
	public String getUrlEncoding() {
		return urlEncoding;
	}
	/**
	 * @return the htmlEncoding
	 */
	public String getHtmlEncoding() {
		return htmlEncoding;
	}
	
	
	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}
	/**
	 * @param standardValue the standardValue to set
	 */
	public void setStandardValue(String standardValue) {
		if (standardValue == null) 
			this.standardValue = "";
		else 
			this.standardValue = standardValue;
	}
	/**
	 * @param mandatory the mandatory to set
	 */
	public void setMandatory(boolean mandatory) {
		this.mandatory = mandatory;
	}
	/**
	 * @param urlEncoding the urlEncoding to set
	 */
	public void setUrlEncoding(String urlEncoding) {
		this.urlEncoding = urlEncoding;
	}
	/**
	 * @param htmlEncoding the htmlEncoding to set
	 */
	public void setHtmlEncoding(String htmlEncoding) {
		this.htmlEncoding = htmlEncoding;
	}
	/**
	 * @param persist the persist to set
	 */
	public void setPersistent(boolean persistent) {
		this.persistent = persistent;
	}
	/**
	 * @return the persist
	 */
	public boolean isPersistent() {
		return persistent;
	}
	/**
	 * @param inputParameterTypeString the type to set
	 */
	public void setType(int type) {
		this.type = type;
	}
	/**
	 * @return the type
	 */
	public int getType() {
		return type;
	}
	/**
	 * @param inputFormat the inputFormat to set
	 */
	public void setInputFormat(String inputFormat) {
		this.inputFormat = inputFormat;
	}
	/**
	 * @return the inputFormat
	 */
	public String getInputFormat() {
		return inputFormat;
	}
	/**
	 * @param persistFormat the persistFormat to set
	 */
	public void setPersistFormat(String persistFormat) {
		this.persistFormat = persistFormat;
	}
	/**
	 * @return the persistFormat
	 */
	public String getPersistFormat() {
		return persistFormat;
	}

}
